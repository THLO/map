command -option:a "data/% ('+|_ : ).txt" -output:b "data/% ('+|_ : )-0.txt"
command -option:a "data/1.txt" -output:b "data/1-1.txt"
command -option:a "data/2.txt" -output:b "data/2-2.txt"
command -option:a "data/3.abc" -output:b "data/3-3.abc"
command -option:a "data/_-# #%.txt" -output:b "data/_-# #%-4.txt"
command -option:a "data/_:.txt" -output:b "data/_:-5.txt"
command -option:a "data/dotext." -output:b "data/dotext-6."
command -option:a "data/noext" -output:b "data/noext-7"
